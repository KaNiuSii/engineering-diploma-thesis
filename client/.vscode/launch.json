{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug KW Chess (Windows)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "cwd": "${workspaceFolder}",
      "args": ["-d", "windows"],
      "flutterMode": "debug"
    },
    {
      "name": "Release KW Chess (Windows)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "cwd": "${workspaceFolder}",
      "args": ["-d", "windows", "--release"],
      "flutterMode": "release"
    }
  ]
}